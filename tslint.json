{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
      "quotemark": [true, "single"],
      "no-console": false,
      "member-access": false,
      "object-literal-sort-keys": false,
      "interface-name": false,
      "member-ordering": false,
      "no-empty": false,
      "no-namespace": false,
      "max-classes-per-file": false
   }
}